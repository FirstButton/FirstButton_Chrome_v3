{
  "manifest_version": 3,
  "name": "FirstButton for Example",
  "version": "2.4",
  "description": "FirstButton for Example",
  "background": {
    "service_worker": "background.js"
  },
  "icons": { 
    "16": "images/icon16.png",
    "19": "images/icon19.png", 
    "32": "images/icon32.png",
    "38": "images/icon38.png",
    "48": "images/icon48.png",               
    "64": "images/icon64.png",             
    "128": "images/icon128.png"},
  "author": "Liquidity Lighthouse, LLC",	
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Liquidity Lighthouse"
  }
}